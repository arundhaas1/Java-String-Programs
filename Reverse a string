      // Reverse a string
   
package com.dhas;
import java.util.Arrays;
import java.util.Locale;

public  class Main {
    public static void main(String[] args) {

        String name="arun";

        for (int i=name.length()-1;i>=0;i--){
            reverseName=reverseName+name.charAt(i);   
        }
       System.out.print(reverseName);
    }
}
